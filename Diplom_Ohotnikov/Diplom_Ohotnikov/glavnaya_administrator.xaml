        <Button x:Name="button1" Content="БД 'Услуги'" HorizontalAlignment="Left" Margin="277,137,0,0" VerticalAlignment="Top" Width="248" Height="45" FontSize="22" Background="Red"  Foreground="White" FontWeight="Bold" Click="button1_Click"/>
        <Button x:Name="button2" Content="БД 'Клиенты'" HorizontalAlignment="Left" Margin="277,216,0,0" VerticalAlignment="Top" Width="248" Height="45" FontSize="22" Background="Red"  Foreground="White" FontWeight="Bold" Click="button2_Click"/>

    </Grid>
</Window>
